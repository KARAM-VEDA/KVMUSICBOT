{
    "name": "★彡KV MUSICBOT彡★",
    "logo": "https://telegra.ph/file/9d135de2809d9c624a78c.jpg",
    "description": "Open-Source bot to play songs in your Telegram's Group Voice Chat. Made with ❤️ By @Karam_0912 & Join @KV_NETWORK for support. Powered by PyTgCalls.",
    "keywords": ["music", "voicechat", "telegram"],
    "repository": "https://github.com/KARAM-VEDA/KVMUSICBOT",
    "stack": "container",
    "env": {
        "SESSION_NAME": {
            "description": "Session string (pyrogram)",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "From @BotFather",
            "required": true
        },
        "BOT_NAME": {
            "description": "Your VC Bot Name.",
            "required": false,
            "value": ""
       },
        "API_ID": {
            "description": "From my.telegram.org/apps",
            "required": true
        },
        "API_HASH": {
            "description": "From my.telegram.org/apps",
            "required": true
        },
        "SUDO_USERS": {
            "description": "Full-access users, counted as admin everywhere, seperated by space.",
            "required": true
        },
        "DURATION_LIMIT": {
            "description": "In minutes!",
            "required": true,
            "value": "10"
        }
    },
    "buildpacks": [
       {
      "url": "heroku/python"
       }
     ]
  }
